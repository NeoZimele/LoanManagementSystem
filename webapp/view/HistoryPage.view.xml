<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="loansLoanManagementSystem.controller.HistoryPage">
	<App>
		<pages>
			<Page title="Application History" backgroundDesign="Solid" showNavButton="true" navButtonPress="onNavBack">
				<content>
				    <Table items="{ path: '/'}" id="tblHistory">
				        <items>
				            <ColumnListItem type="Navigation" press="onObjectHistory">
				                <cells>
				                    <Text text="{ApplicationID}" />
				                    <Text text="28/11/2018" />
				                    <Text text="Submitted"/></cells>
				            </ColumnListItem>
				          
				           </items>
				        <columns>
				            <Column id="__column0">
				                <header>
				                    <Label text="Application ID" id="__label0"/>
				                </header>
				            </Column>
				            <Column id="__column1">
				                <header>
				                    <Label text="Date" id="__label1"/>
				                </header>
				            </Column>
				            <Column id="__column2">
				                <header>
				                    <Label text="Status" id="__label2"/>
				                </header>
				            </Column></columns></Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>