<mvc:View xmlns:u="sap.ui.unified" xmlns:ui="sap.ui.core" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="loansLoanManagementSystem.controller.Registration">
	<App>
		<pages>
			<Page backgroundDesign="Solid" showNavButton="true" navButtonPress="onNavBack" title="Registration Page">
				<content>
					<HBox width="100%" alignContent="Center" alignItems="Center" justifyContent="Center" height="200px">
						<items>
							<Image width="350px" height="200px" src="images/vingca.png"/>
						</items>
					</HBox>
					<sap.ui.layout.form:Form xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true">
						<sap.ui.layout.form:formContainers>
							<sap.ui.layout.form:FormContainer>
								<sap.ui.layout.form:formElements>
										<sap.ui.layout.form:FormElement>
										<sap.ui.layout.form:fields>
											<ComboBox width="80%" id="inpUserType">
												<items>
													<ui:ListItem text="Admin"/>
													<ui:ListItem text="Applicant"/>
													</items>
											</ComboBox>
											<!--<Input id="inpUserType" width="80%"/>-->
										</sap.ui.layout.form:fields>
										<sap.ui.layout.form:label>
											<Label text="Type of user" width="100%" required="true"/>
										</sap.ui.layout.form:label>
									</sap.ui.layout.form:FormElement>
									<sap.ui.layout.form:FormElement>
										<sap.ui.layout.form:fields>
											<Input id="inpName" width="80%"/>
										</sap.ui.layout.form:fields>
										<sap.ui.layout.form:label>
											<Label text="Name" width="100%" required="true"/>
										</sap.ui.layout.form:label>
									</sap.ui.layout.form:FormElement>
									<sap.ui.layout.form:FormElement>
										<sap.ui.layout.form:fields>
											<Input id="inpSurname" width="80%"/>
										</sap.ui.layout.form:fields>
										<sap.ui.layout.form:label>
											<Label text="Surname" width="100%" required="true"/>
										</sap.ui.layout.form:label>
									</sap.ui.layout.form:FormElement>
									<sap.ui.layout.form:FormElement>
										<sap.ui.layout.form:fields>
											<Input id="email" width="80%"/>
										</sap.ui.layout.form:fields>
										<sap.ui.layout.form:label>
											<Label text="Email" width="100%" id="__label3" required="true"/>
										</sap.ui.layout.form:label>
									</sap.ui.layout.form:FormElement>
									<sap.ui.layout.form:FormElement xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:sap.ui.layout.form="sap.ui.layout.form" id="__element0_copy">
										<!--<sap.ui.layout.form:label>-->
										<!--    <Label xmlns="sap.m" text="Contact Number" width="100%" required="true" id="__label5"/>-->
										<!--</sap.ui.layout.form:label>-->
									</sap.ui.layout.form:FormElement>
									<sap.ui.layout.form:FormElement xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:sap.ui.layout.form="sap.ui.layout.form" id="__element0_copy2">
										<sap.ui.layout.form:fields>
											<Input id="username" width="80%"/>
										</sap.ui.layout.form:fields>
										<sap.ui.layout.form:label>
											<Label xmlns="sap.m" text="Username" width="100%" required="true" id="__label6"/>
										</sap.ui.layout.form:label>
									</sap.ui.layout.form:FormElement>
									<sap.ui.layout.form:FormElement xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:sap.ui.layout.form="sap.ui.layout.form" id="__element0_copy3">
										<sap.ui.layout.form:fields>
											<Input id="inpPassword" width="80%" type="Password"/>
										</sap.ui.layout.form:fields>
										<sap.ui.layout.form:label>
											<Label xmlns="sap.m" text="Password" width="100%" required="true" id="__label7"/>
										</sap.ui.layout.form:label>
									</sap.ui.layout.form:FormElement>
									<sap.ui.layout.form:FormElement xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:sap.ui.layout.form="sap.ui.layout.form" id="__element0_copy4">
										<sap.ui.layout.form:fields>
											<Input xmlns="sap.m" width="80%" id="__input0_copy4" type="Password"/>
										</sap.ui.layout.form:fields>
										<sap.ui.layout.form:label>
											<Label xmlns="sap.m" text="Confirm Password" width="100%" required="true" id="__label8"/>
										</sap.ui.layout.form:label>
									</sap.ui.layout.form:FormElement>
								</sap.ui.layout.form:formElements>
								<sap.ui.layout.form:title>
									<core:Title text="Profile Registration" emphasized="true" icon="sap-icon://add-contact"/>
								</sap.ui.layout.form:title>
							</sap.ui.layout.form:FormContainer>
						</sap.ui.layout.form:formContainers>
						<sap.ui.layout.form:layout>
							<sap.ui.layout.form:ResponsiveGridLayout id="__layout0"/>
						</sap.ui.layout.form:layout>
					</sap.ui.layout.form:Form>
				</content>
				<footer>
					<Toolbar width="100%" id="__toolbar1">
						<content>
							<!--<Button text="Back" width="90px" id="__button1" type="Back"/>-->
							<ToolbarSpacer id="__spacer0"/>
							<Button text="Submit" width="100px" icon="sap-icon://accept" type="Reject" press="onSubmitReg"/>
						</content>
					</Toolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>